                <div class="listing-item">
                    <h2 class="listing-item-h2">@((string.IsNullOrWhiteSpace(title) ? w.Name : title))</h2>

                    <div class="listing-item-description">
                        @* Если надо кратко — Summary(w); если нужен полный HTML — Raw *@
                        @Html.Raw(Summary(w) ?? "")
                        @* или замените строкой: @Summary(w) *@
                    </div>

                    <dl class="listing-item-date">
                        <dt>Length:</dt>
                        <dd>@DisplayLength(w)</dd>
                        <dt>Date</dt>
                        <dd>@DisplayDate(w)</dd>
                    </dl>

                    <ul class="listing-item-btns">
                        <li class="listing-item-btn">
                            <a class="btn btn-green btn-218" href="@w.Url()">Learn More</a>
                        </li>
                    </ul>
                </div>
            }
        }
        

    </section>
    @if (totalPages > 1)
    {
        <section class="pagination simple-pagination">
            <ul class="pagination">
                @for (var i = 1; i <= totalPages; i++)
                {
                    var isActive = i == currentPage;
                    <li>
                        <a class="pagination-link @(isActive ? "pagination-link-active" : null)" href="?page=@i">@i</a>
                    </li>
                }
            </ul>
        </section>
    }
    <article class="content-article"></article>

</div>